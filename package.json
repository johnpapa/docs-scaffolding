{
	"name": "docs-scaffolding",
	"displayName": "docs-scaffolding",
	"description": "",
	"version": "0.0.1",
	"publisher": "jw",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scaffoldModule",
				"title": "Learn scaffolding: Create new module"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Docs Scaffolding Extension Configuration",
			"properties": {
				"docs.scaffolding.alias": {
					"type": "string",
					"default": "",
					"description": "Microsoft alias"
				},
				"docs.scaffolding.githubid": {
					"type": "string",
					"default": "",
					"description": "GitHub ID"
				},
				"docs.scaffolding.learn_repo_id": {
					"type": "string",
					"default": "",
					"description": "Default Learn repo"
				},
				"docs.scaffolding.template_repo": {
					"type": "string",
					"default": "https://github.com/MicrosoftDocs/learn-scaffolding/archive/main.zip",
					"description": "Learn template repo"
				}
			}
		},
		"menus": {
			"explorer/context": [
				{
					"command": "scaffoldModule",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.50.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@octokit/rest": "^18.0.6",
		"axios": "^0.19.0",
		"download-git-repo": "^1.0.2",
		"fs-extra": "^9.0.1",
		"replace-in-file": "^6.1.0",
		"vscode-extension-telemetry": "^0.1.1",
		"extract-zip": "^2.0.1",
		"open": "^7.3.0"
	}
}
